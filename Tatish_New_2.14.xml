                      <ab>
                           <lb n="1"/>+ <rs type="invocation" ref="invocation.xml#Θεωτόκε"><expan><abbr>Θ</abbr><ex>ε</ex><abbr>ω
                            <lb n="2" break="no"/>τόκε</abbr></expan> 
